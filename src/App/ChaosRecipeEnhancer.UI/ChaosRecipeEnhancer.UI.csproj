<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<!--Build Settings-->
		<TargetFramework>net8.0-windows</TargetFramework>
		<OutputType>WinExe</OutputType>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<UseWindowsForms>True</UseWindowsForms>
		<UseWPF>true</UseWPF>
		<IsPackable>false</IsPackable>
		<!--Application Name & Version-->
		<AssemblyName>ChaosRecipeEnhancer</AssemblyName>
		<MapFileExtensions>true</MapFileExtensions>
		<UseApplicationTrust>false</UseApplicationTrust>
		<PublishWizardCompleted>true</PublishWizardCompleted>
		<BootstrapperEnabled>true</BootstrapperEnabled>
		<ApplicationVersion>3.26</ApplicationVersion>

		<!--Bump this by 1 for minor patches; Bump by 10 for major patches-->
		<!--Minor Patch Bump: v3.24.1000 -> v3.24.1001; App Revision 10 -> 11-->
		<!--Minor Patch Bump: v3.24.1000 -> v3.24.2000; App Revision 10 -> 20-->
		<ApplicationRevision>10</ApplicationRevision>

		<!--Update Settings-->
		<IsWebBootstrapper>false</IsWebBootstrapper>
		<Install>true</Install>
		<InstallFrom>Disk</InstallFrom>
		<UpdateEnabled>true</UpdateEnabled>
		<UpdateMode>Foreground</UpdateMode>
		<UpdateInterval>1</UpdateInterval>
		<UpdateIntervalUnits>Days</UpdateIntervalUnits>
		<UpdatePeriodically>true</UpdatePeriodically>
		<UpdateRequired>false</UpdateRequired>
		<ImportWindowsDesktopTargets>true</ImportWindowsDesktopTargets>
	</PropertyGroup>
	<PropertyGroup>
		<ApplicationIcon>Assets\Icons\CREIcon.ico</ApplicationIcon>
		<PackageIcon>CREIcon.ico</PackageIcon>
		<StartupObject>ChaosRecipeEnhancer.UI.App</StartupObject>
		<ApplicationManifest>app.manifest</ApplicationManifest>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
	  <NoWarn>1701;1702;CA1416</NoWarn>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
	  <NoWarn>1701;1702;CA1416</NoWarn>
	</PropertyGroup>
